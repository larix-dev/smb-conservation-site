// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
  isAdmin   Boolean   @default(false)
}

model Announcement {
  id     String  @id @default(cuid())
  text   String  @default("")
  colour String  @default("blue")
  active Boolean @default(true)
}

model About {
  id      Int    @id
  content String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model Burial {
  id              Int     @id
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
  content         String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model Feedback {
  id              Int     @id
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
  content         String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model Footer {
  id              Int    @id
  address         String @default("")
  phone           String @default("")
  instagramHandle String @default("")
  facebookHandle  String @default("")
}

model GalleryTag {
  id                     String         @id @default(cuid())
  tagName                String         @default("")
  from_GalleryImage_tags GalleryImage[] @relation("GalleryImage_tags")
}

model GalleryImage {
  id              String       @id @default(cuid())
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
  caption         String       @default("")
  author          String       @default("")
  dateTaken       String
  tags            GalleryTag[] @relation("GalleryImage_tags")
}

model Map {
  id           Int    @id
  content      String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  centreCoords String @default("")
  zoom         Int
}

model Trail {
  id          String @id @default(cuid())
  name        String @default("")
  trailCoords String @default("")
  colour      String @default("#3b82f6")
}

model Privacy {
  id      Int    @id
  content String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model Disclaimer {
  id      Int    @id
  content String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model ProductsServicesPage {
  id      Int    @id
  content String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model Product {
  id              String  @id @default(cuid())
  title           String  @default("")
  urlId           String  @default("")
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
  isService       Boolean @default(false)
  source          String  @default("")
  description     String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model EcosystemPage {
  id Int @id
}

model Organism {
  id                 String  @id @default(cuid())
  name               String  @default("")
  scientificName     String  @default("")
  urlId              String  @default("")
  type               String
  conservationStatus String  @default("Unclassified")
  image_filesize     Int?
  image_extension    String?
  image_width        Int?
  image_height       Int?
  image_id           String?
  description        String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model MailRecipient {
  id    String @id @default(cuid())
  name  String @default("")
  email String @default("")
}
